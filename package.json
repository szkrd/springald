{
  "name": "springald",
  "version": "0.0.2",
  "description": "Simple application launcher, similar to Launchy.",
  "main": "index.html",
  "scripts": {
    "dev": "nw --enable-logging=stderr ."
  },
  "window": {
    "show": false,
    "resizable": false,
    "icon": "assets/icon.png",
    "position": "center",
    "always-on-top": true,
    "visible-on-all-workspaces": true
  },
  "single-instance": true,
  "author": "Szabolcs Kurdi",
  "license": "ISC",
  "dependencies": {
    "osenv": "0.1.4"
  }
}
