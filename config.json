{
  "winWidth": 800,
  "fluxboxMenuFile": "~/.fluxbox/menu",
  "showOnStartup": false,
  "centerOnShow": true,
  "toggleKey": "Command+Q",
  "refreshKey": "F5",
  "centerKey": "Ctrl+C",
  "logicalAndSeparator": " ",
  "includeFiles": [ "\\.(txt|md|html|docx|pdf|jpe?g|png|avi|mkv|mp4)$" ],
  "excludeFiles": [ "^\\._" ],
  "excludedDirs": [ "^node_modules$", "^\\." ],
  "excludeHidSys": true,
  "terminalCommand": "xterm -e %CMD%",
  "appShortcuts": {
    "showItemInFolder": "F"
  },
  "directories": [
    "~/Downloads",
    "~/Pictures",
    "~/Videos"
  ]
}
