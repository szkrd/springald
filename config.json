{
  "theme": "default",
  "winWidth": 800,
  "fluxboxMenuFile": "~/.fluxbox/menu",
  "borderlessWindow": false,
  "showOnStartup": false,
  "centerOnShow": true,
  "autoSelectAll": true,
  "toggleKey": ["Super+`", "CmdOrCtrl+Alt+Shift+`"],
  "logicalAndSeparator": " ",
  "desktopFilesLocation": "/usr/share/applications/",
  "includeFiles": [ "\\.(txt|md|html|docx|pdf|jpe?g|png|avi|mkv|mp4)$" ],
  "excludeFiles": [ "^\\._" ],
  "excludedDirs": [ "^node_modules$", "^\\." ],
  "excludeHidSys": true,
  "terminalCommand": "gnome-terminal -- %CMD%",
  "appShortcuts": {
    "showItemInFolder": "F",
    "launchInTerminal": "T"
  },
  "openWith": {
    "\\.(txt|md)$": "code"
  },
  "directories": [
    "~/Downloads",
    "~/Pictures",
    "~/Videos"
  ]
}
